#!/usr/bin/env ruby

require "bundler/setup"
require "pub_grub"

bar = PubGrub::Package.new "bar" do |p|
  p.add_version "2.0.0"
  p.add_version "1.0.0"
end

foo = PubGrub::Package.new "foo" do |p|
  p.add_version "2.0.0"
  p.add_version "1.1.0"
  p.add_version "1.0.0"
end

require 'irb'
IRB.setup(__FILE__)
workspace = IRB::WorkSpace.new(binding)
IRB::Irb.new(workspace).run(IRB.conf)
